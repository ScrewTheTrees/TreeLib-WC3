{
  "compilerOptions": {
    "strict": true,
    "sourceMap": true,
    "target": "esnext",
    "lib": ["esnext"],
    "outDir": "./src",
    "types": [
      "lua-types/core/coroutine",
      "lua-types/core/global",
      "lua-types/core/math",
      "lua-types/core/metatable",
      "lua-types/core/modules",
      "lua-types/core/string",
      "lua-types/core/table",
      "lua-types/core/debug",
      "lua-types/special/5.3"
    ]
  },

  "tstl": {
    "luaTarget": "5.3",
    "luaLibImport": "inline",
    //"sourceMapTraceback": true,
    "noHeader": true
  }
}