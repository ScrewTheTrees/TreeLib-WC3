{
  "name": "treelib",
  "author": "Fredrik Grimmenhag (ScrewTheTrees)",
  "description": "Quality package.",
  "scripts": {
    "fullRecompileAndRun": "npm run clean && npm run compileTypescript && npm run compileAndRunMap",
    "fullRecompile": "npm run clean && npm run compileTypescript && npm run compileMap",
    "clean": "rimraf src/* && rimraf target/*",
    "compileTypescript": "tstl -p tsconfig.json",
    "compileAndRunMap": "%npm_package_config_ceres% run %npm_package_config_devMap%",
    "compileMap": "%npm_package_config_ceres% build %npm_package_config_devMap%"
  },
  "directories": {
    "lib": "lib"
  },
  "main": "main.ts",
  "dependencies": {
    "typescript": "^3.7.2",
    "typescript-to-lua": "^0.28.1",
    "lua-types": "^2.6.2",
    "rimraf": "^3.0.0"
  },
  "config": {
    "ceres": "tools\\ceres\\ceres.exe",
    "devMap": "TestMap.w3x"
  }
}
